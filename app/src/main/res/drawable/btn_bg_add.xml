<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <solid
                android:color="#CD6155" />
            <stroke
                android:width="1dp"
                android:color="#CD6155" />
            <corners
                android:radius="10dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke
                android:width="0dp"
                android:color="#C0392B" />
            <corners
                android:radius="10dp" />
            <solid
                android:color="#C0392B" />

        </shape>
    </item>
</selector>
